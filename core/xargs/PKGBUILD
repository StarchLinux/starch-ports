pkgname=xargs
pkgver=0
pkgrel=1
pkgdesc=""
arch=('x86_64')
url="http://openbsd.org"
license=('BSD')
groups=('base') 
makedepends=('bmake' 'libbsd')
source=($pkgname.tar.gz)

build() {
  cd $srcdir/$pkgname
  test -z $CC && export CC=cc
  bmake CC=$CC CFLAGS="$CFLAGS -DREG_BASIC=0" LDFLAGS="$LDFLAGS" LDADD='-lbsd'
}

package() {
  export DESTDIR=$pkgdir
  export BINDIR=/bin
  export MANDIR=/usr/share/man
  cd $srcdir/$pkgname
  bmake install
}

sha1sums=('503fe81c7db77601cdd838956971f16ea6a24e84')
sha1sums=('e28e8201b00127e1bc13b69b54909c7a274b9151')
