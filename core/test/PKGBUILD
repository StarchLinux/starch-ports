pkgname=test
pkgver=15635.1
pkgrel=1
pkgdesc="Test Utility"
arch=('x86_64')
url="http://openbsd.org"
license=('BSD')
groups=() 
makedepends=('bmake' 'libbsd')
source=($pkgname.tar.gz)

build() {
  cd $srcdir/$pkgname
  test -z $CC && export CC=cc
  bmake CC=$CC CFLAGS="$CFLAGS -D__dead=" LDFLAGS="$LDFLAGS" LDADD='-lbsd'
}

package() {
  export DESTDIR=$pkgdir
  export BINDIR=/bin
  export MANDIR=/usr/share/man
  cd $srcdir/$pkgname
  bmake install
}

sha1sums=('d83c8a30ac92d43d9a03c6c0c8588fceee02c352')
